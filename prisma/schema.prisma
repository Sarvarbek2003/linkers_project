// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id Int @id @default(autoincrement())
  user_id BigInt?
  first_name String?
  steep Json?
  phone_number String?
  is_customer Boolean?
  is_admin Boolean?
}

model services {
  id Int @id @default(autoincrement())
  service_name String?
}