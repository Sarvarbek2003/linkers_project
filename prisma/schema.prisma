generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id           Int     @id @default(autoincrement())
  user_id      BigInt?
  first_name   String?
  steep        Json?
  phone_number String?
  is_customer  Boolean @default(true)
  is_admin     Boolean @default(false)
}

model masters {
  id                  Int     @id @default(autoincrement())
  user_id             BigInt?
  name                String?
  phone_number        String?
  service_id          Int?
  workshop_name       String?
  address             String?
  landmark            String?
  longtitude          String?
  latitude            String?
  start_time          String?
  end_time            String?
  time_per_cutomer    String?
  rating_count        Int?
  rating              Int?
  is_banned           Boolean @default(false)
  is_verified         Boolean @default(false)
  ban_expiration_time BigInt?
}

model orders {
  id          Int     @id @default(autoincrement())
  customer_id Int?
  master_id   Int?
  rating      Int?
  status      String?
  time        BigInt?
}

model services {
  id           Int     @id @default(autoincrement())
  service_name String?
}
