// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id Int @id @default(autoincrement())
  user_id BigInt?
  

}

model orders {
  id Int @id @default(autoincrement())
  customer_id Int?
  master_id Int?
  status String?
  time BigInt?
  rating Int?

}