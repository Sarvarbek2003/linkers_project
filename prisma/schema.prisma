// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id      Int     @id @default(autoincrement())
  user_id BigInt?
}

model masters {
  id                  Int       @id @default(autoincrement())
  user_id             BigInt?
  name                String?
  phone_number        String?
  service_id          Int?
  workshop_name       String?
  address             String?
  landmark            String?
  longtitude          String?
  latitude            String?
  start_time          DateTime?
  end_time            DateTime?
  time_per_cutomer    String?
  rating_count        Int?
  rating              Int?
  is_banned           Boolean   @default(false)
  ban_expiration_time DateTime?
  is_verified         Boolean   @default(false)
}

model orders {
  id Int @id @default(autoincrement())
  customer_id Int?
  master_id Int?
  status String?
  time BigInt?
  rating Int?

}